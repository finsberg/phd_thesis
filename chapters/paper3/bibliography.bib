  month = may,
  year = {2010},
  note = {00000},
  keywords = {Animals,Biocompatible Materials,Bone Regeneration,Calcium Phosphates,Electron,Male,Microscopy,Osseointegration,Scanning,Swine,X-Ray Diffraction},
  pages = {3552--63},
