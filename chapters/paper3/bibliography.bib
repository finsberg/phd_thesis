@article{McGill2012JCMROJSCMR,
  title = {Reproducibility of In-Vivo Diffusion Tensor Cardiovascular Magnetic Resonance in Hypertrophic Cardiomyopathy.},
  volume = {14},
  issn = {1532-429X},
  doi = {10.1186/1532-429X-14-86},
  abstract = {BACKGROUND: Myocardial disarray is an important histological feature of hypertrophic cardiomyopathy (HCM) which has been studied post-mortem, but its in-vivo prevalence and extent is unknown. Cardiac Diffusion Tensor Imaging (cDTI) provides information on mean intravoxel myocyte orientation and potentially myocardial disarray. Recent technical advances have improved in-vivo cDTI, and the aim of this study was to assess the interstudy reproducibility of quantitative in-vivo cDTI in patients with HCM.\$$\backslash$backslash\$n\$$\backslash$backslash\$nMETHODS AND RESULTS: A stimulated-echo single-shot-EPI sequence with zonal excitation and parallel imaging was implemented. Ten patients with HCM were each scanned on 2 different days. For each scan 3 short axis mid-ventricular slices were acquired with cDTI at end systole. Fractional anisotropy (FA), mean diffusivity (MD), and helix angle (HA) maps were created using a cDTI post-processing platform developed in-house. The mean $\pm$ SD global FA was 0.613 $\pm$ 0.044, MD was 0.750 $\pm$ 0.154 \texttimes{} 10-3 mm2/s and HA was epicardium -34.3 $\pm$ 7.6$^\circ$, mesocardium 3.5 $\pm$ 6.9$^\circ$ and endocardium 38.9 $\pm$ 8.1$^\circ$. Comparison of initial and repeat studies showed global interstudy reproducibility for FA (SD = $\pm$ 0.045, Coefficient of Variation (CoV) = 7.2\%), MD (SD = $\pm$ 0.135 \texttimes{} 10-3 mm2/s, CoV = 18.6\%) and HA (epicardium SD = $\pm$ 4.8$^\circ$; mesocardium SD = $\pm$ 3.4$^\circ$; endocardium SD = $\pm$ 2.9$^\circ$). Reproducibility of FA was superior to MD (p = 0.003). MD was significantly higher in the septum than the reference lateral wall (0.784 $\pm$0.188 vs 0.714 $\pm$0.155 \texttimes{}10-3 mm2/s, p $\backslash$textless0.001) [corrected]. Septal HA was significantly lower than the reference lateral wall in all 3 transmural layers (from -8.3$^\circ$ to -10.4$^\circ$, all p $\backslash$textless 0.001).\$$\backslash$backslash\$n\$$\backslash$backslash\$nCONCLUSIONS: To the best of our knowledge, this is the first study to assess the interstudy reproducibility of DTI in the human HCM heart in-vivo and the largest cDTI study in HCM to date. Our results show good reproducibility of FA, MD and HA which indicates that current technology yields robust in-vivo measurements that have potential clinical value. The interpretation of regional differences in the septum requires further investigation.},
  timestamp = {2017-08-08T20:44:25Z},
  journal = {J. Cardiovasc. Magn. Reson. Off. J. Soc. Cardiovasc. Magn. Reson.},
  author = {McGill, Laura-Ann and Ismail, Tevfik F and Nielles-Vallespin, Sonia and Ferreira, Pedro F. and Scott, Andrew D and Roughton, Michael and Kilner, Philip J and Ho, Siew Y and McCarthy, Karen P and Gatehouse, Peter D and {de Silva}, Ranil and Speier, Peter and Feiweier, Thorsten and Mekkaoui, Choukri and Sosnovik, David E. and Prasad, Sanjay K and Firmin, David N and Pennell, Dudley J.},
  year = {2012},
  keywords = {Aged,Cardiomyopathy,Contrast Media,Contrast Media: diagnostic use,Diffusion Tensor Imaging,Female,Fibrosis,Humans,Hypertrophic,Hypertrophic: diagnosis,Hypertrophic: pathology,Hypertrophic: physiopathology,Left,Male,Middle Aged,Myocardium,Myocardium: pathology,Observer Variation,Predictive Value of Tests,Prospective Studies,Reproducibility of Results,Stroke Volume,Ventricular Function},
  pages = {86},
  pmid = {23259835}
}

@article{Bonanno2015SR,
  title = {Ultra-High-Resolution {{3D}} Imaging of Atherosclerosis in Mice with Synchrotron Differential Phase Contrast: A Proof of Concept Study.},
  volume = {5},
  issn = {2045-2322},
  doi = {10.1038/srep11980},
  abstract = {The goal of this study was to investigate the performance of 3D synchrotron differential phase contrast (DPC) imaging for the visualization of both macroscopic and microscopic aspects of atherosclerosis in the mouse vasculature ex vivo. The hearts and aortas of 2 atherosclerotic and 2 wild-type control mice were scanned with DPC imaging with an isotropic resolution of 15 $\mu$m. The coronary artery vessel walls were segmented in the DPC datasets to assess their thickness, and histological staining was performed at the level of atherosclerotic plaques. The DPC imaging allowed for the visualization of complex structures such as the coronary arteries and their branches, the thin fibrous cap of atherosclerotic plaques as well as the chordae tendineae. The coronary vessel wall thickness ranged from 37.4 $\pm$ 5.6 $\mu$m in proximal coronary arteries to 13.6 $\pm$ 3.3 $\mu$m in distal branches. No consistent differences in coronary vessel wall thickness were detected between the wild-type and atherosclerotic hearts in this proof-of-concept study, although the standard deviation in the atherosclerotic mice was higher in most segments, consistent with the observation of occasional focal vessel wall thickening. Overall, DPC imaging of the cardiovascular system of the mice allowed for a simultaneous detailed 3D morphological assessment of both large structures and microscopic details.},
  timestamp = {2017-08-08T20:44:25Z},
  number = {July},
  journal = {Sci. Rep.},
  author = {Bonanno, Gabriele and Coppo, Simone and Modregger, Peter and Pellegrin, Maxime and Stuber, Annina and Stampanoni, Marco and Mazzolai, Lucia and Stuber, Matthias and {van Heeswijk}, Ruud B},
  year = {2015},
  pages = {11980},
  file = {Bonanno et al. - 2015 - Ultra-high-resolution 3D imaging of atherosclerosis in mice with.pdf:/home/genet/Zotero/Bonanno et al. - 2015 - Ultra-high-resolution 3D imaging of atherosclerosis in mice with.pdf:application/pdf},
  pmid = {26165698}
}

