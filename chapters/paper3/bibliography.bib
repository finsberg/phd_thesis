  keywords = {Animals,Biological,Biomechanics,Computer Simulation,Finite Element Analysis,Heart,Heart: physiology,Models,Shear Strength,Swine},
  pages = {161--73},
  file = {Schmid et al. - 2008 - Myocardial material parameter estimation - a non-homogeneous finite.pdf:/home/genet/Zotero/Schmid et al. - 2008 - Myocardial material parameter estimation - a non-homogeneous finite.pdf:application/pdf},
