  pages = {507--24},
  file = {Pathak et al. - 2008 - The simulation of stress fibre and focal adhesion development in cells.pdf:/home/genet/Zotero/Pathak et al. - 2008 - The simulation of stress fibre and focal adhesion development in cells.pdf:application/pdf},
