  pages = {788--96},
  file = {Rim, Pinsky, van Osdol - 2008 - Using the method of homogenization to calculate the effective.pdf:/home/genet/Zotero/Rim, Pinsky, van Osdol - 2008 - Using the method of homogenization to calculate the effective.pdf:application/pdf},
