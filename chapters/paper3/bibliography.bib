@article{Chaboche1995RA,
  title = {A Continuum Damage Theory with Anisotropic and Unilateral Damage},
  abstract = {Continuum Damage Mechanics offers a simple and attractive framework for describing the progressive deterioration of materials before macrocrack initiation and fracture. A better modelling of actual damaging processes in initially isotropic materials like metals and concrete needs to take into account two factors: The anisotropic nature of the damage, which is related to the direction of the past loading history; The unilateral character of damage, corresponding to the possible closure of defects under compressive loading conditions. In this paper, this discussion is limited to the case of the elastic behaviour of rate independent damaging materials like concrete. A new unilateral damage condition is formulated, which allows the description of active/passive damage effect with anisotropic damage, using either second or fourth order tensors for the damage state variables. Moreover, a simple but general form of the damage rate equation is proposed, which has the potential of describing fairly well the development of the anisotropic damage using a fourth order damage tensor.},
  timestamp = {2017-04-14T20:29:31Z},
