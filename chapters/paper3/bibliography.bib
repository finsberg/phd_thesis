  year = {2013},
  keywords = {Adventitia,Adventitia: metabolism,Adventitia: pathology,Animals,Collagen,Collagen: metabolism,Coronary Vessels,Coronary Vessels: metabolism,Coronary Vessels: pathology,Elastin,Elastin: metabolism,Heart,Heart: physiopathology,Swine},
  pages = {1683--93},
