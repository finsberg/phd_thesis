  keywords = {Endothelium,Hemodynamics,Ion Channels,Ion Channels: physiology,Mechanical,Potassium,Potassium: metabolism,Stress,Vascular,Vascular: cytology,Vascular: metabolism,Vascular: physiology},
  pages = {168--70},
