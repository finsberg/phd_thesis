  pages = {5081--5099},
  file = {Rubin, Bodner - 2002 - A three-dimensional nonlinear model for dissipative response of soft.pdf:/home/genet/Zotero/Rubin, Bodner - 2002 - A three-dimensional nonlinear model for dissipative response of soft.pdf:application/pdf}
