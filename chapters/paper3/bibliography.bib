  month = jul,
  year = {2009},
  note = {00000},
  keywords = {Engineering,Kinetics,Materials Testing,Materials Testing: methods,Mechanical Phenomena,Models,Nanotechnology,Nanotechnology: methods,Theoretical},
  pages = {11484--9},
  file = {Bažant, Le, Bažant - 2009 - Scaling of strength and lifetime probability distributions of.pdf:/home/genet/Zotero/Bažant, Le, Bažant - 2009 - Scaling of strength and lifetime probability distributions of.pdf:application/pdf},
