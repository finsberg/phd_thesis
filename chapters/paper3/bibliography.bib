  keywords = {Focal Adhesions,Mechano-sensitivity,Myosin.,Self-assembly,Stress Fibers},
  pages = {1484--1510},
  file = {Deshpande et al. - 2008 - A bio-mechanical model for coupling cell contractility with focal.pdf:/home/genet/Zotero/Deshpande et al. - 2008 - A bio-mechanical model for coupling cell contractility with focal.pdf:application/pdf}
}

