  pages = {761--788},
  file = {Rossi et al. - 2012 - Orthotropic active strain models for the numerical simulation of.pdf:/home/genet/Zotero/Rossi et al. - 2012 - Orthotropic active strain models for the numerical simulation of.pdf:application/pdf}
