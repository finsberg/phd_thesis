  keywords = {Animals,Cardiovascular,Cell Membrane,Cell Membrane: physiology,Computer Simulation,Electrophysiology,Electrophysiology: methods,Heart,Heart: physiology,Humans,Models},
  pages = {1088--97},
  file = {Sundnes et al. - 2006 - On the computational complexity of the bidomain and the monodomain.pdf:/home/genet/Zotero/Sundnes et al. - 2006 - On the computational complexity of the bidomain and the monodomain.pdf:application/pdf},
