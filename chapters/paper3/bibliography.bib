  number = {Suppl 1},
  journal = {J. Cardiovasc. Magn. Reson.},
  author = {Lamata, Pablo and Hussain, Shazia T and Kutty, Shelby and Steinmetz, Michael and Sohns, Jan M and Fasshauer, Martin and Staab, Wieland and Unterberg-Buchwald, Christina and Lotz, Joachim and Schuster, Andreas},
  month = jan,
  year = {2014},
  note = {00000},
  pages = {P14},
  file = {Lamata et al. - 2014 - Cardiovascular magnetic resonance myocardial feature tracking for the.pdf:/home/genet/Zotero/Lamata et al. - 2014 - Cardiovascular magnetic resonance myocardial feature tracking for the.pdf:application/pdf}
