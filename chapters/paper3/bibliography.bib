  keywords = {Animals,Blood Pressure,Cardiac,Cardiac: transplantation,Cardiomyopathy,Cardiovascular,Dilated,Dilated: physiopathology,Dilated: therapy,Humans,Left,Models,Myocytes,Stroke Volume,Ventricular Function},
  pages = {508--13},
