  pages = {501--22},
  file = {Nash, Panfilov - 2004 - Electromechanical model of excitable tissue to study reentrant cardiac.pdf:/home/genet/Zotero/Nash, Panfilov - 2004 - Electromechanical model of excitable tissue to study reentrant cardiac.pdf:application/pdf},
