  number = {2},
  journal = {Q. Rev. Biophys.},
  author = {K{\"o}rber, Christoph},
  month = may,
  year = {1988},
  note = {00091},
  pages = {229--98},
  file = {Körber - 1988 - Phenomena at the advancing ice-liquid interface - solutes, particles and.pdf:/home/genet/Zotero/Körber - 1988 - Phenomena at the advancing ice-liquid interface - solutes, particles and.pdf:application/pdf},
  pmid = {3043537}
