  month = feb,
  year = {2011},
  note = {00000},
  keywords = {Animals,Biomechanics,Cardiovascular,Computer Simulation,Dogs,Elasticity,Finite Element Analysis,Left,Models,Myocardial Contraction,myocardial infarction,Myocardial Infarction: physiopathology,Myocardial Infarction: therapy,Nonlinear Dynamics,Stroke Volume,Ventricular Function,Ventricular Pressure},
  pages = {82--91},
  file = {Fomovsky et al. - 2011 - Model-based design of mechanical therapies for myocardial infarction.pdf:/home/genet/Zotero/Fomovsky et al. - 2011 - Model-based design of mechanical therapies for myocardial infarction.pdf:application/pdf},
  pmid = {21088945}
