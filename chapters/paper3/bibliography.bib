  month = jan,
  year = {2012},
  keywords = {Action Potentials,Animal,Animals,Cardiovascular,Cellular,Computer Simulation,Disease Models,Feedback,Finite Element Analysis,Heart Conduction System,Heart Conduction System: physiopathology,Left,Left: diagnosis,Left: physiopathology,Magnetic Resonance Imaging,Mechanoreceptors,Mechanoreceptors: metabolism,Mechanotransduction,Models,myocardial infarction,Myocardial Infarction: diagnosis,Myocardial Infarction: physiopathology,Physiological,Sheep,Time Factors,Ventricular Dysfunction,Ventricular Function},
  pages = {H206--14},
  file = {Wall et al. - 2012 - Electromechanical feedback with reduced cellular connectivity alters.pdf:/home/genet/Zotero/Wall et al. - 2012 - Electromechanical feedback with reduced cellular connectivity alters.pdf:application/pdf},
  pmid = {22058157}
