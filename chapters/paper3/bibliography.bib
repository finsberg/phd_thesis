  author = {Waldman, L. K. and Fung, Y. C. and Covell, James W},
  month = jul,
  year = {1985},
  note = {00393},
  keywords = {Animals,Biomechanical Phenomena,Cardiac Volume,Diastole,Dogs,Myocardial Contraction,Systole,Ventricular Function},
  pages = {152--163},
  file = {Waldman, Fung, Covell - 1985 - Transmural myocardial deformation in the canine left ventricle. Normal.pdf:/home/genet/Zotero/Waldman, Fung, Covell - 1985 - Transmural myocardial deformation in the canine left ventricle. Normal.pdf:application/pdf},
