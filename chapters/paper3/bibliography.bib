  number = {6},
  journal = {Ann. Thorac. Surg.},
  author = {Wenk, Jonathan F. and Ge, Liang and Zhang, Zhihong and Mojsejenko, Dimitri and Potter, D. Dean and Tseng, Elaine E. and Guccione, Julius M. and Ratcliffe, Mark B.},
  month = jun,
