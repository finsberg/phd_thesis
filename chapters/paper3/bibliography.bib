}

@article{Knobloch2014MRM,
  title = {Mapping Mean and Fluctuating Velocities by {{Bayesian}} Multipoint {{MR}} Velocity Encoding-Validation against {{3D}} Particle Tracking Velocimetry},
  volume = {71},
  issn = {15222594},
  doi = {10.1002/mrm.24785},
  abstract = {PURPOSE: To validate Bayesian multipoint MR velocity encoding against particle tracking velocimetry for measuring velocity vector fields and fluctuating velocities in a realistic aortic model.\$$\backslash$backslash\$n\$$\backslash$backslash\$nMETHODS: An elastic cast of a human aortic arch equipped with an 80 or 64\% stenotic section was driven by a pulsatile pump. Peak velocities and peak turbulent kinetic energies of more than 3 m/s and 1000 J/m(3) could be generated. Velocity vector fields and fluctuating velocities were assessed using Bayesian multipoint MR velocity encoding with varying numbers of velocity encoding points and particle tracking velocimetry in the ascending aorta.\$$\backslash$backslash\$n\$$\backslash$backslash\$nRESULTS: Velocities and turbulent kinetic energies measured with 5-fold k-t undersampled 10-point MR velocity encoding and particle tracking velocimetry were found to reveal good correlation with mean differences of -4.8 $\pm$ 13.3 cm/s and r(2) = 0.98 for velocities and -21.8 $\pm$ 53.9 J/m(3) and r(2) = 0.98 for turbulent kinetic energies, respectively. Three-dimensional velocity patterns of fast flow downstream of the stenoses and regions of elevated velocity fluctuations were found to agree well.\$$\backslash$backslash\$n\$$\backslash$backslash\$nCONCLUSION: Accelerated Bayesian multipoint MR velocity encoding has been demonstrated to be accurate for assessing mean and fluctuating velocities against the reference standard particle tracking velocimetry. The MR method holds considerable potential to map velocity vector fields and turbulent kinetic energies in clinically feasible exam times of $\backslash$textless15 min.},
