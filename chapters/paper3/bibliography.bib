  keywords = {BEATING LEFT-VENTRICLE,CANINE LEFT-VENTRICLE,CARDIAC ALLOGRAFT-REJECTION,Cardiac mechanics,deformation,Heart,Humans,INTRAMYOCARDIAL MARKERS,model,MUSCLE SHELLS,NONLINEAR-THEORY,RAT LEFT-VENTRICLE,torsion,twist,ventricle},
  pages = {745--752}
