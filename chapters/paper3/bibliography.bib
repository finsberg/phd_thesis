  keywords = {Animal,Animals,Cardiomyopathy,Dilated,Dilated: surgery,Disease Models,Dogs,Equipment Design,Equipment Safety,Finite Element Analysis,Heart-Assist Devices,Heart Function Tests,Materials Testing,Mechanical,Random Allocation,Sensitivity and Specificity,Stress,Ventricular Remodeling,Ventricular Remodeling: physiology},
  pages = {2022--7},
