@article{Winklhofer2014ER,
  title = {Post-Mortem Cardiac Diffusion Tensor Imaging: Detection of Myocardial Infarction and Remodeling of Myofiber Architecture.},
  volume = {24},
  issn = {1432-1084},
  doi = {10.1007/s00330-014-3322-7},
  abstract = {OBJECTIVES: To investigate the accuracy of post-mortem diffusion tensor imaging (DTI) for the detection of myocardial infarction (MI) and to demonstrate the feasibility of helix angle (HA) calculation to study remodelling of myofibre architecture. METHODS: Cardiac DTI was performed in 26 deceased subjects prior to autopsy for medicolegal reasons. Fractional anisotropy (FA) and mean diffusivity (MD) were determined. Accuracy was calculated on per-segment (AHA classification), per-territory, and per-patient basis, with pathology as reference standard. HAs were calculated and compared between healthy segments and those with MI. RESULTS: Autopsy demonstrated MI in 61/440 segments (13.9 \%) in 12/26 deceased subjects. Healthy myocardial segments had significantly higher FA (p $\backslash$textless 0.01) and lower MD (p $\backslash$textless 0.001) compared to segments with MI. Multivariate logistic regression demonstrated that FA (p $\backslash$textless 0.10) and MD (p = 0.01) with the covariate post-mortem time (p $\backslash$textless 0.01) predicted MI with an accuracy of 0.73. Analysis of HA distribution demonstrated remodelling of myofibre architecture, with significant differences between healthy segments and segments with chronic (p $\backslash$textless 0.001) but not with acute MI (p $\backslash$textgreater 0.05). CONCLUSIONS: Post-mortem cardiac DTI enablesdifferentiation between healthy and infarcted myocardial segments by means of FA and MD. HA assessment allows for the demonstration of remodelling of myofibre architecture following chronic MI. KEY POINTS: \textbullet{} DTI enables post-mortem detection of myocardial infarction with good accuracy. \textbullet{} A decrease in right-handed helical fibre indicates myofibre remodelling following chronic myocardial infarction. \textbullet{} DTI allows for ruling out myocardial infarction by means of FA. \textbullet{} Post-mortem DTI may represent a valuable screening tool in forensic investigations.},
  timestamp = {2017-08-08T20:44:25Z},
  number = {11},
  journal = {Eur. Radiol.},
  author = {Winklhofer, Sebastian and Stoeck, Christian T. and Berger, Nicole and Thali, Michael and Manka, Robert and Kozerke, Sebastian and Alkadhi, Hatem and Stolzmann, Paul},
  month = nov,
  year = {2014},
  pages = {2810--8},
  pmid = {25052077}
