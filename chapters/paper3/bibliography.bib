  pages = {H2650--9},
  file = {Dokos et al. - 2002 - Shear properties of passive ventricular myocardium.pdf:/home/genet/Zotero/Dokos et al. - 2002 - Shear properties of passive ventricular myocardium.pdf:application/pdf},
