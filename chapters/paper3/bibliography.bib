  keywords = {Algorithms,Computer-Assisted,Heart Diseases,Heart Diseases: diagnosis,Heart Diseases: pathology,Heart Diseases: physiopathology,Humans,Image Interpretation,Imaging,Magnetic Resonance Imaging,Myocardial Contraction,Myocardium,Myocardium: pathology,Predictive Value of Tests,Prognosis,Severity of Illness Index,Stroke Volume,Three-Dimensional,Ventricular Function},
  pages = {36},
  file = {Ibrahim - 2011 - Myocardial tagging by cardiovascular magnetic resonance - evolution of.pdf:/home/genet/Zotero/Ibrahim - 2011 - Myocardial tagging by cardiovascular magnetic resonance - evolution of.pdf:application/pdf},
