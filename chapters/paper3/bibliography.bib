  year = {2012},
  note = {00000},
  keywords = {Atlases,diffusion weighted imaging,Heart},
  pages = {1436--1447},
  file = {Lombaert et al. - 2012 - Human atlas of the cardiac fiber architecture - Study on a healthy.pdf:/home/genet/Zotero/Lombaert et al. - 2012 - Human atlas of the cardiac fiber architecture - Study on a healthy.pdf:application/pdf},
