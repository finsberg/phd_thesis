@article{Peerlings1998MCM,
  title = {Gradient-Enhanced Damage Modelling of Concrete Fracture},
  volume = {3},
  abstract = {Classical continuum damage theory for quasi-brittle fracture exhibits an extreme sensitivity to the fineness and orientation of the spatial discretization in finite element simulations. This sensitivity is caused by the fact that the mathematical description becomes ill-posed at a certain level of accumulated damage. The ill-posedness can be removed by the use of a gradient-enhanced damage model, In this model, higher-order deformation gradients give rise to a non-local effect, which regularizes the localization of deformation and thus renders numerical analyses mesh-objective. The mesh objectivity of the gradient-enhanced damage approach is demonstrated by the application to two concrete fracture experiments: a double-edge notched bar subjected to a uniaxial, tensile load and a single-edge notched beam under anti-symmetric four-point loading. Both the initiation and the propagation of damage can be simulated. Particularly the latter aspect calls for an appropriate definition of the strain measure which governs the evolution of damage.},
  timestamp = {2017-08-08T20:44:25Z},
  number = {4},
  journal = {Mech. Cohesive-Frict. Mater.},
