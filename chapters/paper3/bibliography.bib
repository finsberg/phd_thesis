  number = {6},
  journal = {J. Am. Coll. Cardiol.},
  author = {Jackson, Benjamin M. and Gorman, Joseph H. and Moainie, Sina L. and Guy, T. Sloane and Narula, Navneet and Narula, Jagat and St. John-Sutton, Martin G. and Edmunds, L. Henry and Gorman, Robert C.},
