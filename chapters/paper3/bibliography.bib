  keywords = {Animals,Endocardium,Endocardium: anatomy \& histology,Endocardium: physiology,Heart,Heart: anatomy \& histology,Heart: physiology,Left,Left: physiology,Male,Myocardial Contraction,Myocardial Contraction: physiology,Pericardium,Pericardium: anatomy \& histology,Pericardium: physiology,Sheep,Ventricular Function},
