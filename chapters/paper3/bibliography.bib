  pages = {612--625},
  file = {Sermesant, Delingette, Ayache - 2006 - An electromechanical model of the heart for image analysis and.pdf:/home/genet/Zotero/Sermesant, Delingette, Ayache - 2006 - An electromechanical model of the heart for image analysis and.pdf:application/pdf},
  groups = {Cardiac,Cardiac,Cardiac,DataAssimilation,DataAssimilation,Cardiac,Cardiac},
