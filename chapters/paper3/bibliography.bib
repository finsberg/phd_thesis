  pages = {1689--1709},
  file = {Le Guyader, Vese - 2011 - A combined segmentation and registration framework with a nonlinear.pdf:/home/genet/Zotero/Le Guyader, Vese - 2011 - A combined segmentation and registration framework with a nonlinear.pdf:application/pdf}
}

