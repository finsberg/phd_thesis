@phdthesis{DeMelo-Loseille2010b,
  title = {Pr{\'e}vision de La Dur{\'e}e de Vie Des Composites {\`a} Matrice C{\'e}ramique Auto Cicatrisante, En Fatigue Statique, {\`a} Haute Temp{\'e}rature ($\backslash$textless 800$^\circ${{C}})},
  abstract = {La rupture diff{\'e}r{\'e}e d'un composite SiC/SiC a {\'e}t{\'e} {\'e}tudi{\'e}e en fatigue statique, sous air, aux temp{\'e}ratures interm{\'e}diaires (500$^\circ$C-800$^\circ$C). Les r{\'e}sultats exp{\'e}rimentaux (dur{\'e}e de vie, loi de comportement en traction, d{\'e}formation) et les fractographies ont permis d'identifier les modes de rupture de fibre qui interviennent dans la rupture diff{\'e}r{\'e}e. Un mod{\`e}le m{\'e}canique probabiliste multi{\'e}chelle a {\'e}t{\'e} d{\'e}velopp{\'e} pour simuler le comportement en fatigue et pr{\'e}voir la dur{\'e}e de vie. La microstructure est d{\'e}crite par des distributions statistiques identifi{\'e}es {\`a} partir des r{\'e}sultats de l'{\'e}tude fractographique. Des diagrammes d'endurance du composite en fatigue ont {\'e}t{\'e} calcul{\'e}s pour pr{\'e}voir la dur{\'e}e de vie. Ces derniers confirment que la tenue du composite est dict{\'e}e par les fils. La mod{\'e}lisation montre que la microstructure joue un r{\^o}le d{\'e}terminant sur la dur{\'e}e de vie et sa variabilit{\'e}. Des relations microstructures-propri{\'e}t{\'e}s sont {\'e}tablies. Le lien entre contrainte r{\'e}siduelle et dur{\'e}e de vie est {\'e}galement examin{\'e}. Une approche fiabiliste sur les {\'e}chantillons {\`a} information faible est men{\'e}e {\`a} l'aide de l'inf{\'e}rence bay{\'e}sienne. Les r{\'e}sultats concordent avec l'approche m{\'e}canique.},
  timestamp = {2017-04-14T20:29:31Z},
  school = {Bordeaux I},
  author = {De Melo-Loseille, Olivier},
  year = {2010},
  file = {De Melo-Loseille - 2010 - Prévision de la durée de vie des composites à matrice céramique auto.pdf:/home/genet/Zotero/De Melo-Loseille - 2010 - Prévision de la durée de vie des composites à matrice céramique auto.pdf:application/pdf}
}

