  number = {2},
  journal = {J. Thorac. Cardiovasc. Surg.},
  author = {Frydrychowicz, Alex and Berger, Alexander and Russe, Maximilian F and Stalder, Aur{\'e}lien F and Harloff, Andreas and Dittrich, Sven and Hennig, J{\"u}rgen and Langer, Mathias and Markl, Michael},
