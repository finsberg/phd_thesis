  pages = {2243--54},
  file = {Bayer et al. - 2012 - A novel rule-based algorithm for assigning myocardial fiber orientation_2.pdf:/home/genet/Zotero/Bayer et al. - 2012 - A novel rule-based algorithm for assigning myocardial fiber orientation_2.pdf:application/pdf;Bayer et al. - 2012 - A novel rule-based algorithm for assigning myocardial fiber orientation.pdf:/home/genet/Zotero/Bayer et al. - 2012 - A novel rule-based algorithm for assigning myocardial fiber orientation.pdf:application/pdf},
