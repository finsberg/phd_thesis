  author = {Kirk, Jonathan A. and Kass, David A.},
  month = aug,
  year = {2013},
  keywords = {Cardiac Resynchronization Therapy,Heart Failure,Heart Failure: metabolism,Heart Failure: physiopathology,Heart Failure: therapy,Humans,Ventricular Dysfunction,Ventricular Dysfunction: metabolism,Ventricular Dysfunction: physiopathology,Ventricular Dysfunction: therapy},
  pages = {765--76},
  file = {Kirk, Kass - 2013 - Electromechanical dyssynchrony and resynchronization of the failing.pdf:/home/genet/Zotero/Kirk, Kass - 2013 - Electromechanical dyssynchrony and resynchronization of the failing.pdf:application/pdf},
