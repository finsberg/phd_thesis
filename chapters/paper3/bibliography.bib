  keywords = {Animals,Blood Pressure,Finite Element Analysis,Heart Ventricles,Heart Ventricles: physiopathology,Imaging,Magnetic Resonance Imaging,Male,Mechanical,Myocardial Contraction,myocardial infarction,Myocardial Infarction: physiopathology,Sheep,Stress,Three-Dimensional},
  file = {Lee et al. - 2011 - A Novel Method for Quantifying In-Vivo Regional Left Ventricular.pdf:/home/genet/Zotero/Lee et al. - 2011 - A Novel Method for Quantifying In-Vivo Regional Left Ventricular.pdf:application/pdf},
  doi = {10.1115/1.4004995},
  pmid = {22010752}
}

