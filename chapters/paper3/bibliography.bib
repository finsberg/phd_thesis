@article{Reese1996JMRB,
  title = {Measuring {{Diffusion}} in the {{Presence}} of {{Material Strain}}},
  volume = {112},
  abstract = {Material strain during the course of diffusion encoding by MRI will in general change the observed diffusional signal losses. These changes will occur even when the material returns cyclically to its initial location during the diffusion-evolution period. This effect derives from the modification of the local spatial modulation k of spin phase within a sample by a material deformation \$$\backslash$backslash\$font\$$\backslash$backslash\$serif=cmss10 at 10pt \$$\backslash$backslashhbox\{$\backslash$backslashserif F\}\$ as k \textendash$\backslash$textgreater \$$\backslash$backslashhbox\{$\backslash$backslashserif F\}\^\{$\backslash$backslashrm 1T\}\$ k, resulting in an observed diffusion tensor \$$\backslash$backslashhbox\{$\backslash$backslashserif D\}\^\{$\backslash$backslashrm obs\} = \{\{1\}$\backslash$backslashover\{$\backslash$backslashDelta\}\} $\backslash$backslashint\^\{$\backslash$backslashDelta\}\_0 $\backslash$backslashhbox\{$\backslash$backslashserif U\}(t)\^\{-1\} $\backslash$backslashhbox\{$\backslash$backslashserif DU\}(t)\^\{-1\}dt,\$ where \$$\backslash$backslashhbox\{$\backslash$backslashserif\{U\}\}\$ is the material stretch tensor. For example, when a material is compressed during pulsed-gradient diffusion encoding, the compression acts to increase the attenuation due to diffusion just as if a larger gradient were used. By using a simple gelatin phantom, the existence of this effect is demonstrated, and an effective method for its correction based on an MRI mapping of the material strain is presented. This correction is particularly relevant for inferring myofiber structure in the beating in vivo human heart, since the measurement of \$$\backslash$backslashhbox\{$\backslash$backslashserif\{D\}\}\$ is perturbed by the deformation of myocardium during the heart's contraction.},
