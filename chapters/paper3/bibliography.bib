}

@article{Lesne1993RA,
  title = {Modeling of Irreversible Damage-Induced Strains in Brittle Elastic Composites},
  issn = {0034-1223},
  abstract = {This paper is devoted to the modeling of the nonlinear damaged elastic properties of a class of ceramic matrix composite materials. In the first part, the structure of these composites is described along with their principal characteristics and the macroscopic pheneomena characterizing their damage. In the second part, we propose a model including the main phenomena observed, especially irreversible strains. These irreversible strains are described without reference to the classical plasticity or viscoplasticity formalism. In the third part, we point out both the necessity and advantages of using a fourth-rank tensor for the description of damage. ``Nondiagonal'' terms are needed in this tensor (D(ijkl)(ij not-equal kl)=0) in order to be able to describe the microscopic phenomena through a macroscopic formulation. The fourth part gives an application of this formalism to a ceramic matrix composite, and shows the advantages of the model.},
