  keywords = {Anatomic,Animals,Cardiac,Cardiac: cytology,Cell Shape,Cell Size,Endocardium,Endocardium: cytology,Heart,Heart: anatomy \& histology,Heart Ventricles,Heart Ventricles: anatomy \& histology,Heart Ventricles: cytology,Models,Myocardium,Myocardium: cytology,Myocytes,Paraffin,Pericardium,Pericardium: cytology,Staining and Labeling,Swine},
